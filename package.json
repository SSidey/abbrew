{
  "name": "abbrew",
  "version": "2.0.0",
  "description": "CSS compiler for the Abbrew system",
  "scripts": {
    "dev": "vite",
    "build": "gulp build",
    "build:clean": "node ./utils/packs.mjs package clean",
    "build:db": "node ./utils/packs.mjs package pack",
    "build:json": "node ./utils/packs.mjs package unpack",
    "preview": "vite preview",
    "compile": "gulp css",
    "watch": "gulp watch",
    "gulp": "gulp",
    "link": "gulp link",
    "build2": "sass src/scss/abbrew.scss css/abbrew.css --style=expanded --no-source-map",
    "watch2": "sass src/scss/abbrew.scss css/abbrew.css --style=expanded --source-map --watch"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Abde",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "chalk": "^3.0.0",
    "foundryvtt-simple-calendar": "^2.4.18",
    "fs-extra": "^9.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "sass": "^1.53.0",
    "yargs": "^15.4.1"
  },
  "dependencies": {
    "@yaireo/tagify": "^4.24.0",
    "vite": "^5.2.8"
  }
}
